{
  "name": "cypress-layout-inspector",
  "version": "0.0.0-semantically-released",
  "description": "Simple utility to provide layout testing functionality to Cypress",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist/",
    "build": "rollup -c",
    "lint": "eslint src",
    "test": "cypress run",
    "test:debug": "cypress open",
    "format": "prettier {src,src/**,cypress/integration/**}/*.{js,html} --write",
    "doctoc": "npx doctoc --maxlevel 3 README.md",
    "prepare": "npm run build",
    "prepublishOnly": "npm run doctoc",
    "postinstall": "node ./scripts/postinstall.js || exit 0"
  },
  "files": [
    "dist",
    "scripts",
    "add-support.js"
  ],
  "keywords": [
    "testing",
    "cypress",
    "ui",
    "dom",
    "functional"
  ],
  "author": "Matthew Simpson <hello@msmp.me> (https://msmp.me)",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "cypress": "^6.2.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.10.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "husky": "^4.3.0",
    "lint-staged": "^10.4.0",
    "prettier": "^2.1.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.29.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "eslint",
      "prettier --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/msmps/cypress-layout-inspector"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
